{
    "manifest_version": 2,
    "name": "HuronOSBookmarksSync",
    "version": "1.0.10",
    "description": "Syncs bookmarks from the huronOS directives file so thah every bookmark that is in the directives appears in the bookmarks bar.",
    "permissions": [
        "bookmarks",
        "storage",
        "nativeMessaging"
    ],
    "background": {
        "scripts": [
            "browser-polyfill.js",
            "background.js"
        ],
        "persistent": true
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "bookmarks@huronos.org",
            "strict_min_version": "57.0"
        }
    },
    "key": ""
}